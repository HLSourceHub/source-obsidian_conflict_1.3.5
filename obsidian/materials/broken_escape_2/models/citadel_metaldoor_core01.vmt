"vertexlitgeneric"
{
	"$basetexture" "metal/citadel_metaldoor_core01"
	"$surfaceprop" "metal"
	"%keywords" "combine"

	"$envmap" "env_cubemap"
	"$envmaptint" "[ .56 .67 .75 ]"
	"$envmapcontrast" 1
	"$envmapsaturation" 1

	"vertexlitgeneric_dx9"
	{
		"$bumpmap" "metal/metalwall077a_normal"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ .09 .1 .12]"
	}

	"vertexlitgeneric_dx8"
	{
		"$bumpmap" "metal/metalwall077a_normal"
		"$nodiffusebumplighting" 1
		"$normalmapalphaenvmapmask" 1
	}

	"vertexlitgeneric_nobump_dx8"
	{
		"$basetexture" "metal/citadel_metaldoor_core01"
		"$basealphaenvmapmask" 1
	}
	
	"vertexlitgeneric_hdr_dx9"
	{
		// put hdr override stuff here
		"$bumpmap" "metal/metalwall077a_normal"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ .09 .1 .12]"
	}
}
